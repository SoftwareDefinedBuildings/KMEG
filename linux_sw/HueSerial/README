require package :
	python 2.7 or python 2.x
	pip
	pyserial

INSTALL PACKAGE  (Ubuntu or Debian)
	apt-get install python-pip
	pip install pyserial
	pip install beautifulhue

INSTALL PACKAGE  (Ubuntu or Debian)
	yum install python-pip
	pip install pyserial
	pip install beautifulhue
